  {
    "title": "multiple_rules",
    "rules": [
      {
        "property": "b",
        "operator": "[in[",
        "operand": [
          23,
          2
        ],
        "type": "time_of_day"
      },
      {
        "property": "a",
        "operator": "[in[",
        "operand": [
          2,
          5
        ],
        "type": "day_of_week"
      },
      {
        "property": "c",
        "operator": "<",
        "operand": 2
      }
    ],
    "expectation": {
      "string": "'b' in [23:00, 02:00[ and 'a' from Wed to Fri and 'c' < 2"
    }
  }
